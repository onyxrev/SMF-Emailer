service_names: 
  - pkgsrc/rabbitmq:default
  - pkgsrc/epmd:default

interval: 60

mail:
  from: from@example.com
  to:
    - to@example.com
    - also@example.com
  subject: SMF services down!
  delivery_method: smtp
  settings:
    address: mail.example.com
    domain: example.com
    port: 25
    user_name: <username>
    password: <password>
    enable_starttls_auto: true
    authentication: plain
